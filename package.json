		"postcss": "^8.4.19",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.6.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.4.3",
		"swiper": "^8.4.5",
		"tailwindcss": "^3.2.4"
	},
	"devDependencies": {
		"@types/react": "^18.0.24",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@vitejs/plugin-react": "^2.2.0",
